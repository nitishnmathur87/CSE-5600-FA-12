    <!-- http://smartplatforms.org/terms/api/calls#search_records -->

    <owl:NamedIndividual rdf:about="&calls;search_records">
        <rdf:type rdf:resource="&api;Call"/>
        <api:path>/records/search</api:path>
        <rdfs:comment>Get an RDF graph of sp:Demographics elements for all patients that match the query.  Matching treats family_name and given_name as the *beginning* of a name.  For instance given_name=&apos;J&apos; matches /^J/i and thus matchs &apos;Josh&apos;. Date of birth is an ISO8601 string like &quot;2008-03-21&quot;; gender is &quot;male&quot; or &quot;female&quot;.  Gender, date_of_birth, zipcode, and medical_record_number must match exactly.
	</rdfs:comment>
        <api:clientMethodName>search_records</api:clientMethodName>
        <api:httpMethod>GET</api:httpMethod>
        <api:category>container</api:category>
        <api:cardinality>multiple</api:cardinality>
        <api:target rdf:resource="http://smartplatforms.org/terms#Demographics"/>
        <api:hasParameter rdf:resource="&parameters;date_of_birth"/>
        <api:hasParameter rdf:resource="&parameters;family_name"/>
        <api:hasParameter rdf:resource="&parameters;gender"/>
        <api:hasParameter rdf:resource="&parameters;given_name"/>
        <api:hasParameter rdf:resource="&parameters;medical_record_number"/>
        <api:hasParameter rdf:resource="&parameters;zipcode"/>
    </owl:NamedIndividual>
    


