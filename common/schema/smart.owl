        </owl:annotatedTarget>
    </owl:Axiom>
    <owl:Axiom>
        <rdfs:comment>Date on which allergy ended as an ISO-8601 string.</rdfs:comment>
        <owl:annotatedSource rdf:resource="http://smartplatforms.org/terms#Allergy"/>
        <owl:annotatedProperty rdf:resource="&rdfs;subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartplatforms.org/terms#endDate"/>
                <owl:allValuesFrom rdf:resource="&rdfs;Literal"/>
            </owl:Restriction>
        </owl:annotatedTarget>
